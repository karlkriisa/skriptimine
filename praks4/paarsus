#!/bin/bash

# Kasutab kasutaja antud parameetrit ning
# m채채rab, kas see on paaris v천i paaritu

if [ $# -ne 1 ]
then
    echo "Kaivita see skript koos arvuga mille paarsus soovid kontrollida. N채iteks: ./paarsus 3"
else
    if [ $(($1 % 2)) -eq 0 ]
    then
        echo "$1 on paaris"
    else
        echo "$1 on paaritu"
    fi
fi



